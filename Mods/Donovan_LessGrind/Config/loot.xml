<config>
  <!-- Add acid to cupboards -->
  <append xpath="/lootcontainers/lootgroup[@name='cupboard']">
    <item name="resourceAcid" count="1,2" prob="0.05"/>
  </append>

  <!-- increase number of Eggs in refrigerators -->
  <set xpath="/lootcontainers/lootgroup[@name='coldfood']/item[@name='foodEgg']/@count">1,12</set>
</config>
